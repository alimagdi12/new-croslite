                            <form action="/cart" method="post" class="me-2">
                              <button class="btn border border-secondary rounded-pill px-3 " type="submit"><i class="fa fa-shopping-bag me-2 color"></i>Add to Bag</button>
                              <input type="hidden" name="productId" value="<%= product._id %>">
                            </form>
                            <% } %>
                            <a href="/products/<%= product._id %>" class="btn border border-secondary rounded-pill px-3" style="color: #679d06;"><i class="fa fa-shopping-bag me-2" style="color: #679d06;"></i> Details</a>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <% c++; %>
                  <% } %>
                  <% } else { %>
                  <h1>No Products Found!</h1>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
