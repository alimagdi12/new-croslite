        <div class="tab-content">
          <div id="tab-1" class="tab-pane fade show p-0 active">
            <div class="row g-4">
              <div class="col-lg-12">
                <div class="row g-4">
                  <% let c = 0; %>
                  <% if (prods.length > 0 || prods.length > 4) { %>
                  <% for (let product of prods) { %>
                  <% if (c >= 4) break; %>
